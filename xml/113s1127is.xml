					<paragraph id="ID4d6a7d74d04c4e16b087fe54c9b9d998">
            <enum>(9)</enum>
            <subparagraph commented="no" display-inline="yes-display-inline" id="idB33FB9914BEA408886933A9EBDFFC5EC">
              <enum>(A)</enum>
