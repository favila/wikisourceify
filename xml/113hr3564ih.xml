              <external-xref legal-doc="usc" parsable-cite="usc/43/407">
            43 U.S.C. 407
          </external-xref> note</cato:entity-ref>; <external-xref legal-doc="public-law" parsable-cite="pl/111/11">
              <cato:entity-ref entity-type="public-law" value="public-law/111/11">Public Law 111–11</cato:entity-ref>
