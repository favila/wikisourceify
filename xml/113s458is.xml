            <clause id="id9592372cb5c54e4abb1f3be1d01ed509">
              <enum>(iii)</enum>
              <text>by striking<cato:entity-ref xmlns:cato="http://namespaces.cato.org/catoxml" entity-type="act" value="Food and Nutrition Act of 2008/s:5/ss:k/p:4">paragraph (4)</cato:entity-ref>.</text>
            </clause>
