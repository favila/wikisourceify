        <enum>(b)</enum>
        <header>Purposes and
			 definitions</header>
				<paragraph id="H68A41F2448B2443B9D28060925C53771">
          <enum>(1)</enum>
          <text display-inline="yes-display-inline">
            <cato:entity xmlns:cato="http://namespaces.cato.org/catoxml" entity-type="law-citation">
              <cato:entity-ref value="Elementary and Secondary Education Act of 1965/s:2201/ss:a/p:3" entity-type="act">Section 2201(a)(3)</cato:entity-ref> (<cato:entity-ref value="usc/20/6661/a/3" entity-type="uscode">
