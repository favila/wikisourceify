					<table align-to-level="section" blank-lines-before="1" colsep="1" frame="none" line-rules="hor-ver" rowsep="0" rule-weights="0.0.0.0.4.17" table-template-name="Duty Suspension, 8 Column" table-type="exception-quote">
						<tgroup block-style="1" cols="8" grid-typeface="1.1" rowsep="0" thead-tbody-ldg-size="0.7.9">
              <colspec coldef="txt-no-ldr-no-spread" colname="column1" colwidth="8pts" min-data-value="8"/>
              <colspec coldef="txt-no-ldr-no-spread" colname="column2" colwidth="62.25pt" min-data-value="39"/>
              <colspec coldef="txt" colname="column3" colwidth="100.50pt" min-data-value="60"/>
              <colspec coldef="txt-no-ldr-no-spread" colname="column4" colwidth="33.00pt" min-data-value="35"/>
              <colspec coldef="txt-no-ldr-no-spread" colname="column5" colwidth="60.75pt" min-data-value="35"/>
              <colspec coldef="txt-no-ldr-no-spread" colname="column6" colwidth="61.50pt" min-data-value="35"/>
